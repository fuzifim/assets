{
    "name": "sayboot/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "sayboot",
        "sayboot cms",
        "sayboot platform",
        "sayboot assets"
    ],
    "homepage": "https://sayboot.com",
    "authors": [
        {
            "name": "Thanh Nguyen",
            "email": "fuzifim@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "laravel/framework": "^5.6|^6|^7|^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SayBoot\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SayBoot\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SayBoot\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "SayBoot\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
